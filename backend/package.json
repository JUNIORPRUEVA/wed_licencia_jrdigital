{
  "name": "@fulltech/api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node --experimental-specifier-resolution=node dist/src/index.js",
    "typecheck": "prisma generate && tsc -p tsconfig.json --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "dotenv -e .env -- prisma migrate deploy",
    "db:migrate:dev": "dotenv -e .env -- prisma migrate dev",
    "db:seed": "dotenv -e .env -- tsx prisma/seed.ts",
    "db:reset": "dotenv -e .env -- prisma migrate reset",
    "db:studio": "prisma studio"
  },
  "prisma": {
    "seed": "dotenv -e .env -- tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fulltech/crypto": "*",
    "@fulltech/shared": "*",
    "@prisma/client": "^6.3.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "dotenv-cli": "^11.0.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.1.0",
    "jose": "^5.9.6",
    "morgan": "^1.10.0",
    "nanoid": "^5.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "prisma": "^6.3.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3"
  }
}
